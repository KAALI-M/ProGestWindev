             controls :
              -
                name : BTN_Se_Connecter
                identifier : 0x211824af276ae187
                internal_properties : 
                type : 4
                code_elements :
                 type_code : 4
                 p_codes :
                  -
                    code : |1+
                     //Nouvelle Logique
                     
                     buffCrypté est un buffer
                     
                     
                     	SI SAI_Utilisateur.Valeur = "" ALORS
                     		Info ("Veuillez saisir votre Login Utilisateur" )
                     	SINON
                     		SI MotdePasseCompteEnCoursDeSaisi = 0 ALORS
                     			HLitRecherchePremier(GPU_Utilisateur,Login,SAI_Utilisateur.Valeur)
                     			SI Pas HTrouve(GPU_Utilisateur) ALORS
                     				SI TentativesCnxRestantes > 0 ALORS
                     					TentativesCnxRestantes--
                     					Erreur("Votre Login n'existe pas ", "il vous reste "+ TentativesCnxRestantes +  " tentative (s)")
                     				SINON
                     					Erreur("Votre Login n'existe pas ")
                     					Ferme(FEN_Connexion)
                     				Fin
                     			Sinon
                     				SI  GPU_Utilisateur.CompteActif = 0  ALORS
                     					Erreur("Votre compte est désactivé veuillez contacter votre Admin")
                     				SINON
                     					SI GPU_Utilisateur.MotPasseASaisir ALORS
                     						Info("Veuillez saisir le mot de passe pour votre compte ")
                     						MotdePasseCompteEnCoursDeSaisi	= 1
                     						SAI_ConfMotDePasse.Visible		= 1
                     					Sinon
                     						si SAI_MotDePasse = "" ALORS
                     							Info("Veuillez saisir votre Mot de Passe")
                     						Sinon
                     							buffCrypté = DecrypteMotdePasse((GPU_Utilisateur.MotPasse))
                     							
                     							    
                     							Si SAI_MotDePasse = buffCrypté ALORS
                     								ToastAffiche("Bonjour " +GPU_Utilisateur.Nom + " " + GPU_Utilisateur.Prenom,300,cvBas,chCentre)
                     								IDUtilisateurLogin = GPU_Utilisateur.IDUtilisateur
                     								SI INT_RememberMe = 1 ALORS
                     									SauveParamètre("LoginUser",SAI_Utilisateur)
                     									SauveParamètre("LoginPSWD",SAI_MotDePasse)
                     									SauveParamètre("LoginRemember",INT_RememberMe)
                     								FIN
                     								ferme(FEN_Connexion)
                     								
                     							sinon
                     								si TentativesCnxRestantes > 1 ALORS
                     									TentativesCnxRestantes--
                     									Erreur("Mot de Pass Incorrect "+ "il vous reste "+ TentativesCnxRestantes +  " tentative (s)")
                     									
                     								Sinon
                     									Erreur("Mot de Pass Incorrect ")
                     									ferme(FEN_Connexion)
                     								FIN
                     								
                     							FIN
                     						FIN
                     					FIN
                     					
                     				FIN
                     			FIN
                     		SINON  //mot de passe à définir
                     			SI SAI_MotDePasse.Valeur <>SAI_ConfMotDePasse.Valeur ALORS
                     				Erreur( "les mots de passe ne correspondent pas")
                     			SINON  
                     				HLitRecherchePremier(GPU_Utilisateur,Login,SAI_Utilisateur.Valeur)
                     				SI HTrouve(GPU_Utilisateur) ALORS
                     					GPU_Utilisateur.MotPasse = EncrypteMotDePasse(SAI_MotDePasse)
                     					GPU_Utilisateur.MotPasseASaisir=0
                     					HModifie(GPU_Utilisateur)
                     					
                     					ToastAffiche("Bonjour " + GPU_Utilisateur.Nom + " " + GPU_Utilisateur.Prenom,300,cvBas,chCentre)
                     					IDUtilisateurLogin = GPU_Utilisateur.IDUtilisateur
                     					Si INT_RememberMe = 1 ALORS
                     						SauveParamètre("LoginUser",SAI_Utilisateur)
                     						SauveParamètre("LoginPSWD",SAI_MotDePasse)
                     						SauveParamètre("LoginRemember",INT_RememberMe)
                     					FIN
                     
                     					ferme(FEN_Connexion)
                     				FIN
                     			FIN
                     	FIN
                     	FIN
                     
                     
                     //Ancienne Logique
                     // utilisateur connecté 
                     //SI RmemberMe = 1 ET loginUtilisateur <> SAI_Utilisateur ALORS  //modification de Login Utilisateur 
                     //	
                     //	SI loginUtilisateur <> "" et LoginMotdepass <> ""  ALORS
                     //			IDUtilisateurLogin =  Login(SAI_Utilisateur,SAI_MotDePasse)
                     //			SI  IDUtilisateurLogin > 0 ALORS
                     //	//			Temporisation(100, tempoDessin)
                     //	//			OuvreSoeur(FEN_ProGest)
                     //	//			Temporisation(100, tempoDessin)
                     //				si loginUtilisateur <> SAI_Utilisateur ALORS
                     //					SauveParamètre("LoginUser",SAI_Utilisateur)
                     //				FIN
                     //				SI LoginMotdepass <> SAI_MotDePasse ALORS
                     //					SauveParamètre("LoginPSWD",SAI_MotDePasse)
                     //				FIN
                     //				si RmemberMe <> INT_RememberMe.Valeur ALORS
                     //					SauveParamètre("LoginRemember",INT_RememberMe)
                     //				FIN			
                     //				Ferme(FEN_Connexion)
                     //			fin
                     //	FIN
                     //sinon 
                     //	IDUtilisateurLogin =  Login(SAI_Utilisateur,SAI_MotDePasse)
                     //	SI  IDUtilisateurLogin> 0 ALORS
                     //		SauveParamètre("LoginUser",SAI_Utilisateur)
                     //		SauveParamètre("LoginPSWD",SAI_MotDePasse)
                     //		SauveParamètre("LoginRemember",INT_RememberMe)
                     //		// ouverture fenêtre principal
                     ////		Temporisation(100, tempoDessin)
                     ////		OuvreSoeur(FEN_ProGest)
                     ////		Temporisation(100, tempoDessin)
                     //		Ferme(FEN_Connexion)
                     //	FIN
                     //	
                     //FIN
                     
                     	
                     
                     
                     
                     
                     
                     
                     
