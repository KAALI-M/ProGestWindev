             controls :
              -
                name : BTN_CréerCompte
                identifier : 0x21cf1d9a235542c9
                internal_properties : 
                type : 4
                code_elements :
                 type_code : 4
                 p_codes :
                  -
                    code : |1+
                     
                     
                     HLitRecherchePremier(GPU_Utilisateur,Login,SAI_Pseudo)
                     si HTrouve(GPU_Utilisateur) ALORS
                     	Info("L'utilisateur '"+ SAI_Pseudo + "' existe déjà !" ,"Veuillez choisir un autre nom d'utilisateur")
                     	retour
                     FIN
                     SI SAI_MotDePasse2 = "" ALORS
                     	Info("Veuillez saisir un mot de passe !")
                     	retour
                     FIN
                     si SAI_Nom2 = "" ou SAI_Prénom2 =" "ALORS
                     	info("Veuillez saisir le nom et le prénom !")
                     	retour
                     FIN
                     si SAI_MotDePasse2 <> SAI_ConfMotDePasse1 ALORS
                     	Info("les mots de passes ne sont pas les mêmes!")
                     	RETOUR
                     FIN
                     GPU_Utilisateur.login = SAI_Pseudo
                     
                     GPU_Utilisateur.MotPasse = EncrypteMotDePasse(SAI_MotDePasse2)
                     GPU_Utilisateur.Nom = SAI_nom2
                     GPU_Utilisateur.Prénom = SAI_Prénom2
                     
                     
                     Si UtilisaeurAcréer= 1 alors
                     	GPU_Utilisateur.CompteActif = 1
                     	GPU_Utilisateur.Interne =1
                     	GPU_Utilisateur.MotPasseASaisir= 0
                     sinon
                     	GPU_Utilisateur.CompteActif = 0
                     FIN
                     
                     
                     Si HAjoute(GPU_Utilisateur) ALORS
                     	SI UtilisaeurAcréer= 1 ALORS
                     		HLitRecherchePremier(Modules,NomModule,ModulesApp.AdministrationOrganisation..Valeur)
                     		si Htrouve(Modules) ALORS
                     			 
                     			ConfigurationDroitsAccès.idGroupeOuUtilisateurs = GPU_Utilisateur.IDUtilisateur
                     			ConfigurationDroitsAccès.estGroupe = 0
                     			ConfigurationDroitsAccès.IDModules = Modules.IDModules
                     			ConfigurationDroitsAccès.Creation            = 1
                     			ConfigurationDroitsAccès.Lecture             = 1
                     			ConfigurationDroitsAccès.Modification        = 1
                     			ConfigurationDroitsAccès.Suppression         = 1
                     			ConfigurationDroitsAccès.Administration      = 1
