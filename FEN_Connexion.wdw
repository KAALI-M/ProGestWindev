  type_code : 1
  p_codes :
   -
     code : |1-
      PROCEDURE MaFenêtre()
      //connexion Serveur
       Server , Utilisat, MdP,nomBDD est une chaine = (ChargeParamètre("Server") , ChargeParamètre("User"),ChargeParamètre("MotdePass"), ChargeParamètre("BDD"))
      
      
      SAI_ConnexServeur = Server
      SAI_ConnexUtilisateur = Utilisat
      SAI_ConnexMotDePasse = MdP
      SAI_ConnexBDD = nomBDD
      
      
      
      //Login
      
      
      loginUtilisateur est une chaine = ""
      LoginMotdepass est une chaine =""
      RmemberMe est un booléen =0 
      RmemberMe= ChargeParamètre("LoginRemember")
      TentativesCnxRestantes est un entier = 3
      MotdePasseCompteEnCoursDeSaisi est un booléen = 0
      UtilisaeurAcréer est un booléen =0
      
      // a faire: cryptage des MdPass
   -
     code : |1+
      Onglet1 = 2
      
      SI  Server <> "" ET   Utilisat <> "" ET  MdP<>"" ET  nomBDD <>"" ALORS
      	SI Connexion_HFSQL(Server,Utilisat,MdP,nomBDD) = 1
      		LIB_NomBD			= "Connecté à BDD :"  + MaConnexion.BaseDeDonnées
      		Forme1.CouleurFond	= VertClair
      		SI HNbEnr(GPU_Utilisateur) =0 alors
      			UtilisaeurAcréer =1 
      		sinon
      			//RememberMe
      			loginUtilisateur	= ChargeParamètre("LoginUser","")
      			LoginMotdepass		= ChargeParamètre("LoginPSWD","")
      			SI INT_RememberMe = 1 ALORS
      				SI loginUtilisateur <> "" ET LoginMotdepass <> "" ALORS
      					SAI_Utilisateur	= loginUtilisateur 
      					SAI_MotDePasse	= LoginMotdepass
      				FIN
      			FIN
      			Onglet1				= 2 
      		FIN
      			
      	SINON 
      		LIB_NomBD			= "Pas de connexion"
      		Forme1.CouleurFond	= RougeClair
      		Onglet1				= 1
      	FIN
      FIN
      
      SI UtilisaeurAcréer = 1 ALORS
      	Onglet1[3].Visible	= 1 // permettre la création du premier 
      	Onglet1				= 3 
      	ToastAffiche("veuillez renseigner les informatiosn du compte admin",toastLong,cvBas,chCentre)
      FIN
      
      
      //a supprimer 
      //ExécuteTraitement(BTN_Se_Connecter,trtClic)
      
      
      
     type : 34
   -
     code : |1+
      
      renvoyer IDUtilisateurLogin
