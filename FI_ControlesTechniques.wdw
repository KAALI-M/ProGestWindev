           code : |1+
            id est un entier sur 8 octets
            SI TableOccurrence(TABLEH_BaseControlesTechniques)> nNbreLigne  ALORS
            	
            	SI COL_Indice = 0 ALORS
            		COL_Indice = TableSelect(TABLEH_BaseControlesTechniques)*10
            	FIN
            		
            	ControlesTechniques.IDControle	= COL_IDBaseControle
            	ControlesTechniques.Désignation					= COL_Désignation
            	ControlesTechniques.Indice 						= COL_Indice
            	ControlesTechniques.IdArchiveDocs				= COL_IDArchiveDocs
            	ControlesTechniques.IDControleParent		= COL_IDBaseControleParent
            	
            	HAjoute(ControlesTechniques)
            SINON 
            	HLitRecherchePremier(ControlesTechniques,IDControle,COL_IDBaseControle)
            	
            	SI HTrouve() = Vrai ALORS
            		ControlesTechniques.IDControle = COL_IDBaseControle
            		ControlesTechniques.Désignation = COL_Désignation
            		ControlesTechniques.Indice 						= COL_Indice
            		ControlesTechniques.IdArchiveDocs = COL_IDArchiveDocs
            		ControlesTechniques.IDControleParent = COL_IDBaseControleParent
            		
            		
            		HModifie(ControlesTechniques)
            		
            		
            		id = COL_IDBaseControle
            	
            	FIN
            FIN
