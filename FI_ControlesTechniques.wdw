      // necessite que la requete REQ_Délais_Projets oit executé avant
      procédure AjoutFils(LOCAL nIdActuel est un entier sur 8 octets, Local nNumActuel est un entier = Null )
      
      bFilsexiste est un booléen = 0//retourné pour indiquer si des fils d'un id  donné ont été ajoutés (existent)
      nPosActuel est un entier
      nPositionFils est un entier
      
      //ajout de l'élément actuel
      
      nPosActuel = TableAjouteFils(TABLEH_BaseControlesTechniques,nNumActuel,REQ_BaseControlesTechniques.IDControle,REQ_BaseControlesTechniques.Indice,REQ_BaseControlesTechniques.Désignation,REQ_BaseControlesTechniques.IdArchiveDocs,"", REQ_BaseControlesTechniques.IDControleParent)
