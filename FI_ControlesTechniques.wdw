       identifier : 0x21051b410753e861
       internal_properties : 
       type : 8
       code_elements :
        type_code : 8
        p_codes :
         -
           code : |1+
            id est un entier sur 8 octets
            	// avance la ligne actuelle d'un niveau (recherche de laigne précedent ayant le mme niveau et la choisi comme père)
            	i	est un entier	//parcours des selections
            	N	est un entier	= TableSelectOccurrence(TABLEH_BaseControlesTechniques)
            	j	est un entier	// parcours recherche des précedent
            	SI N>0 ALORS
                     i=1
            			
            			//recherche enrgistrement précendet ayant le mme IDparent 
            			j=TableSelect(TABLEH_BaseControlesTechniques,i)-1
            			si j>0 alors
            				TANTQUE TABLEH_BaseControlesTechniques[j].COL_IDBaseControleParent <> TABLEH_BaseControlesTechniques[TableSelect(TABLEH_BaseControlesTechniques,i)].COL_IDBaseControleParent ET j>0
            					j-=1
            					SI j=0 ALORS
            						RETOUR
            					FIN
            				FIN
            			FIN
            			
            			SI j>0 ALORS
            				
            				HLitRecherchePremier(ControlesTechniques,IDControle,TABLEH_BaseControlesTechniques[TableSelect(TABLEH_BaseControlesTechniques,i)].COL_IDBaseControle)
            				SI HTrouve(ControlesTechniques) ALORS
            										ControlesTechniques.IDControleParent= TABLEH_BaseControlesTechniques[j].COL_IDBaseControle
            					HModifie(ControlesTechniques)
            					id = COL_IDBaseControle
            					TableAffiche(TABLEH_BaseControlesTechniques,taInit)
            					//reselction de la ligne 
            					TableSelectMoins(TABLEH_BaseControlesTechniques)
            					TableSelectPlus(TABLEH_BaseControlesTechniques,TableCherche(COL_IDBaseControle,id))
            	
            				
            				FIN
            				
            			FIN
            	
            	FIN
