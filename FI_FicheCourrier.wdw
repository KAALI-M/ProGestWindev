         	GR_FicheCourrierTraitementAdministration.Grisé		= PAS poliAccèsTraitement.DroitsAccès.DroitsAccès.Administration
         
         sinon //doc courrier réponse 
         	GR_SAISIECOURRIER.Visible						= 1
         	//GR_SAISIECOURRIER.Visible						= 1
         	//**Traitement 
         	IMG_DocGénération.Visible = Vrai
         	GR_SAISIECOURRIER.Visible							= 1
         	//GR_SAISIECOURRIER.Grisé							= pas poliAccèsSaisie.DroitsAccès.Accès 
         	GR_AFFECTATIONCOURRIER.Visible						= 1
         	//GR_AFFECTATIONCOURRIER.Grisé						= Pas poliAccèsAffect.DroitsAccès.Accès
         	GR_TRAISTEMENTCOURRIER.Visible						= 1 //poliAccèsTraitement.DroitsAccès.Accès
         	//GR_TRAISTEMENTCOURRIER.Grisé					= 0
         	
         	GR_FicheCourrierTraitementCréation.Grisé			= PAS poliAccèsTraitement.DroitsAccès.DroitsAccès.Creation
         	GR_FicheCourrierTraitementLecture.Grisé				= PAS poliAccèsTraitement.DroitsAccès.DroitsAccès.Lecture
         	GR_FicheCourrierTraitementModification.Grisé		= PAS poliAccèsTraitement.DroitsAccès.DroitsAccès.Modification
