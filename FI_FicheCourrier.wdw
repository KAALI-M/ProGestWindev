      				FIN
      	
      							
      				PJCourrier.IDCourriers		= oCourrier.m_nIDCourriers
      				PJCourrier.IdArchiveDocs = resSauveFichier
      				PJCourrier.Titre = TABLE_PJ_courrier.COL_Intitulé
      				PJCourrier.Qté = TABLE_PJ_courrier.COL_Qté
      				
      				HAjoute(PJCourrier)
      			FIN
      			//PJ supprimée
      			SI TABLE_PJ_courrier.COL_Suprimé = 1 ALORS
      				HLitRecherchePremier(PJCourrier,IDCourriersIdArchiveDocs,HConstruitValClé(PJCourrier,IDCourriersIdArchiveDocs,TABLE_PJ_courrier.COL_IDCourriers,TABLE_PJ_courrier.COL_IDArchiveDocs))
      				si HTrouve(PJCourrier) ALORS
      					HSupprime(PJCourrier)
      				FIN
      			FIN		
      			//PJ modifiée 
      			SI TABLE_PJ_courrier.COL_Modifié = 1 ALORS
      				
      				HLitRecherche(PJCourrier,IDPJCourrier,TABLE_PJ_courrier.COL_IDPJCourrier)
      				si HTrouve(PJCourrier) ALORS
      					//chrgement du fichierChargé 
      					SI TABLE_PJ_courrier.COL_IDArchiveDocs >0 ALORS
      						n est un entier sur 8 octets =TABLE_PJ_courrier.COL_IDArchiveDocs
