  procedures :
   -
     name : SauveFicheCourrier
     procedure_id : 1300064971366951518
     type_code : 14
     code : |1-
      procédure SauveFicheCourrier()
      // Renvoi "" si pas d'erreur sinon elle renvoi les erreurs
      bErreur		est un booléen	= 0 
      sMsgErreur	est une chaine	= ""
      
      // Vérification des champs
      //SI COMBO_EntitéExpDest.valeur = 0 ALORS 
      //	Info("Veuillez sélectionner un Exp/destinataire !")
      //	RepriseSaisie(COMBO_EntitéExpDest)
      //FIN
      
      
      SI COMBO_TypeCourrier.valeur = 0  ALORS
      	Info("Veuillez sélectionner le type de courrier")
      	RepriseSaisie(COMBO_TypeCourrier)
      FIN
      SI COMBO_TypeDocument.Valeur = 0  ALORS
      	Info("Veuillez sélectionner la nature du courrier")
      	RepriseSaisie(COMBO_TypeDocument)
      FIN
      si SAI_Réf = "" et oCourrier.m_bEstUnProjetRéponse=0 ALORS
      	Info("Veuillez saisir la référence du courrier")
      	RepriseSaisie(SAI_Réf)
      FIN
      SI SAI_Objet = "" ALORS
      	Info("Veuillez saisir l'objet du courrier")
      	RepriseSaisie(SAI_Objet)
      FIN
      SI SAI_Dte_Courr = "" ALORS
      	Info("Veuillez saisir la date du courrier")
      	RepriseSaisie(SAI_Dte_Courr)
      FIN
      SI SAI_Dte_Récep = "" ALORS
      	Info("Veuillez saisir la de réception du courrier")
      	RepriseSaisie(SAI_Dte_Récep)
      FIN
      SI TableOccurrence(TABLE_CourrierExp_Dest) = 0  ALORS
      	Info("Veuillez ajouter un destinatire/expidieteur ")
      	RepriseSaisie(TABLE_CourrierExp_Dest)
      FIN
      
      //SI COMBO_Action = 0 ALORS
      //	Info("Veuillez sélectionner l'action demandée pour le courrier")
      //	RepriseSaisie(COMBO_Action)
      //FIN
      
      	si SAI_Dte_lImite= "" ET oCourrier.m_bEstUnProjetRéponse=0 ALORS
      		Info("Veuillez saisir la date limite de traitement du courrier ")
      		RepriseSaisie(SAI_Dte_lImite)
      	FIN
      	si COMBO_Priorité=0 ET oCourrier.m_bEstUnProjetRéponse=0 ALORS
      		Info("Veuillez sélectionner la priorité du courrier ")
      		RepriseSaisie(COMBO_Priorité)
      	FIN
      
      si DocCourrierChargé.estvide =vrai  ET oCourrier.m_bEstUnProjetRéponse=0 ALORS
      	Info("Veuillez charger le fichier du courrier ! ")
      	RepriseSaisie(FRM_SansNom6)
      FIN
      
      //MAJ du Courrier
      si DocCourrierModifié() ALORS
      	SauvegardeDocCourrier() // sauvegarde du document ouvert dans l’éditeur de document soit dans l'archive si idArchive soit en local dans chemin
      FIN
      SI GR_FICHECOURRIER_estModifiée.Modifié = Vrai  alors    
      	SI  oCourrier.m_nIDCourriers > 0 ALORS //modif courrier  
      		HLitRecherchePremier(Courriers,IDCourriers,oCourrier.m_nIDCourriers)
      		SI HTrouve (Courriers) ALORS
      			//Chargement des modification
      	
      			MémoireVersFichier(oCourrier,Courriers)
      			oCourrier.m_nIDEntitésAdmin = COMBO_ChargéAffaire.ValeurMémorisée
      			
      			SI DocCourrierChargé.estVide =Faux ALORS 
      				resSauveFichier est un entier
      				SI COMBO_TypeCourrier.ValeurAffichée = "arrivé" ALORS
      					resSauveFichier= DocCourrierChargé.SauveFichierDansArchive(SAI_Fichier,SAI_Réf,ChaîneVersDate(SAI_Dte_Courr),COMBO_TypeDocument.ValeurMémorisée,COMBO_Projet.ValeurMémorisée,oCourrier.m_nIDOuvrages,TABLE_CourrierExp_Dest[1].COL_IDEntitéExpDest)
      				FIN
      				
      				SI COMBO_TypeCourrier.ValeurAffichée = "départ" ALORS
      					resSauveFichier= DocCourrierChargé.SauveFichierDansArchive(SAI_Fichier,SAI_Réf,ChaîneVersDate(SAI_Dte_Courr),COMBO_TypeDocument.ValeurMémorisée,COMBO_Projet.ValeurMémorisée,oCourrier.m_nIDOuvrages,0)				
      				FIN
      				
      				SELON resSauveFichier
      					CAS >= 0
      						// rien
      						Courriers.IDArchiveDocs = resSauveFichier
      		
      					CAS -2  // doublon
      						sMsgErreur += ["- "] + "Un fichier de même nom existe déjà dans la BDD !"
      					CAS -1
      						sMsgErreur += ["- "] + "Le fichier sélectionné n'a pas pu être sauvegardé dans la BDD!"
      				FIN
      				
      				
      //				afficheDocCourrier(ONG_FicheCourrier,LibOngletDocCourrier,"FI_EditeurDocuments",DocCourrierChargé.GetCheminFichier())
      			FIN	
      			SI PAS HModifie(Courriers) ALORS
      				bErreur = 1
      				sMsgErreur += ["- "] + "Le courrier (id:"+Courriers.IDCourriers + ") n'a pas pu être modifié dans la BDD"
      			SINON
      				
      			FIN	
      			
      		SINON 
      			bErreur = 1
      			sMsgErreur += ["- "] + "Le courrier (id:"+Courriers.IDCourriers + ") n’existe pas dans la bas de donnés !"
      		FIN
      	FIN
      //Nouvel courrier	
      	SI  oCourrier.m_nIDCourriers = 0 ALORS
      		oCourrier.m_nIDChargéAff = Utilisateur.m_nIDUtilisateur
      		ocourrier.m_nIDCreatedBy = Utilisateur.m_nIDUtilisateur
      		Ocourrier.m_nIDEntitésAdmin = utilisateur.EntitéUtilisateurActuel.m_nIDEntitésOrg
      		
      		MémoireVersFichier(oCourrier,Courriers)
      		//Sauvegarde éventuel du fichier dans l'archive et chargement de l'id du fichier sélectionné
      		SI DocCourrierChargé.estVide = Faux ALORS
      			
      			resSauveFichier est un entier
      			SI COMBO_TypeCourrier.ValeurAffichée = "arrivé" ALORS
      				resSauveFichier= DocCourrierChargé.SauveFichierDansArchive(SAI_Fichier,SAI_Réf,ChaîneVersDate(SAI_Dte_Courr),COMBO_TypeDocument.ValeurMémorisée,COMBO_Projet.ValeurMémorisée,oCourrier.m_nIDOuvrages,TABLE_CourrierExp_Dest[1].COL_IDEntitéExpDest)
      			FIN
      			SI COMBO_TypeCourrier.ValeurAffichée = "départ" ALORS
      				resSauveFichier= DocCourrierChargé.SauveFichierDansArchive(SAI_Fichier,SAI_Réf,ChaîneVersDate(SAI_Dte_Courr),COMBO_TypeDocument.ValeurMémorisée,COMBO_Projet.ValeurMémorisée,oCourrier.m_nIDOuvrages,0)				
      			FIN
      			SELON resSauveFichier
