      				FIN
      				
      			fin
      		FIN
      //		REQ_PJ_courrier.ParamIDCourriers = oCourrier.m_nIDCourriers
      
      		
      		
      	//Sauvegarde des Projets Réponse
      	TABLE_Réponses_courrier.AffichageActif = Faux
      	TABLE_Réponses_courrier.DésactiveFiltre()
      	
      	CourrierChargé2	est un CFichierChargé
      	resSauveFichier2	est un entier sans signe sur 8 octets
      	
      	POUR TOUT LIGNE DE TABLE_Réponses_courrier
      		
      		//pj ajoutée
      		SI TABLE_Réponses_courrier.COL_Ajouté = 1 ALORS
      			//chargement du fichierChargé 
