      				Courriers.IDArchiveDocs			=  resSauveFichier2
      				HModifie(Courriers)
      			
      			FIN
      			
      		FIN
      		//PJ supprimée
      		SI TABLE_Réponses_courrier.COL_Suprimé = 1 ALORS
      			HLitRecherche(Courriers,IDCourriers,TABLE_Réponses_courrier.COL_IDCourriers)
      			SI HTrouve(Courriers) ALORS
      				HSupprime(Courriers)
      			FIN
      		FIN		
      		//PJ modifiée 
      		SI TABLE_Réponses_courrier.COL_Modifié = 1 ALORS
      			
      			HLitRecherche(Courriers,IDCourriers,TABLE_Réponses_courrier.COL_IDCourriers)
      			SI HTrouve(Courriers) ALORS
      				//chrgement du fichierChargé 
      				
      				SI TABLE_Réponses_courrier.COL_IDArchiveDocs >0 ALORS
      					n est un entier sur 8 octets =TABLE_Réponses_courrier.COL_IDArchiveDocs
      					CourrierChargé2.ChargeFichierParID(n )
