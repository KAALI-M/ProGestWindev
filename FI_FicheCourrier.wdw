       name : IMG_DocGénération
       identifier : 0x24ecedb306f4b7e4
       internal_properties : 
       type : 8
       code_elements :
        type_code : 8
        p_codes :
         -
           code : |1+
            //3 : Générer un Doc. a part
            //1 : &Remplacer
            //2 : &Annuler
            ChemFichier est un chaîne =""
            SELON Dialogue(<§?14ECEE9905A83841§>)
            	// Générer un Doc. a part
            	CAS 3
            		si GR_FICHECOURRIER_ESTMODIFIEE.modifié = vrai ALORS
            			info ("des modifications sont apportées au courrier. celle-ci ne seront pas prise en compte dans la génération de document ")
            		FIN
            		SELON oCourrier.m_sTypeDoc
            			CAS "lettre", "fax", "bordereau"
            				ChemFichier = Génère_Courrier (oCourrier) 
            				afficheDocCourrier(ONG_FicheCourrier,"Courrier Généré",FI_EditeurDocuments,ChemFichier)
            			autres cas 
            		fin
            		
            	// &Remplacer
            	CAS 1
            		SI GR_FICHECOURRIER_ESTMODIFIEE.Modifié = Vrai ALORS
            			Info ("des modifications sont apportées au courrier. celle-ci ne seront pas prise en compte dans la génération de document ")
            		FIN
            		SELON oCourrier.m_sTypeDoc
            			CAS "lettre", "fax", "bordereau"
            				ChemFichier = Génère_Courrier(oCourrier) 
            				SI ChemFichier <> "" ALORS
            					DocCourrierChargé.ChargeFichierExterneTemporaire(ChemFichier)
            					
            					oCourrier.DocCourrier.m_sNom	= fExtraitChemin(ChemFichier,fFichier + fExtension)
            					SAI_Fichier						=  oCourrier.DocCourrier.m_sNom
            					afficheDocCourrier(ONG_FicheCourrier,LibOngletDocCourrier,"FI_EditeurDocuments",DocCourrierChargé.GetCheminFichier())
            					
            					GR_FICHECOURRIER_ESTMODIFIEE..Modifié	= Vrai // le champ intitulé a été modifié
            					SAI_Fichier.Modifié						= Vrai
            				FIN
            			AUTRES CAS 
            		FIN
            		
            		
            	// &Annuler
            	CAS 2
            		
            FIN
            
            
           type : 18
