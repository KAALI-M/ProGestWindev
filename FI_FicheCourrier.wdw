      				SI TABLE_Réponses_courrier.COL_ChemFichierA_OneDrive <> "" ALORS
      					CourrierChargé2.ChargeFichierExterneTemporaire(TABLE_Réponses_courrier.COL_ChemFichierA_OneDrive)
      				FIN
      				//
      				
      				resSauveFichier2				= CourrierChargé2.SauveFichierDansArchive(fConstruitNomFichier(COMBO_Ouvrages.ValeurAffichée+"_" +"Projet Rép. ID "+Courriers.IDCourriers +"_Parent réf "+ COMBO_RéfParent.ValeurAffichée+fExtraitChemin(TABLE_Réponses_courrier.COL_ChemFichierExt,fExtension)) ,"","","",COMBO_Projet.ValeurMémorisée,oCourrier.m_nIDOuvrages,0)
      				
      				
