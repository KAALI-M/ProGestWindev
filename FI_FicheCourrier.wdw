     type : 458752
   -
     name : afficheDocCourrier
     procedure_id : 1303420057847537097
     type_code : 14
     code : |1-
      procédure afficheDocCourrier(Onglet est un champ, LibeléChamp est une chaine,FenetreInterne est une chaine <nom de fenêtre>,*)
      i			est un entier	= 1
      ResChamp	est une chaîne
      ResChamp = EnumèreChamp(Onglet, i)
      TANTQUE ResChamp <> "" 
      	SI Onglet[ResChamp].Libellé = LibeléChamp	 ALORS
      		SORTIR
      	FIN
      	i++
      	ResChamp = EnumèreChamp(Onglet, i)
      FIN
      
      SI ResChamp ="" ALORS  //aucun champ du mme nom n'a été trouvé
      	WL.OngletOuvre(Onglet,LibeléChamp,FenetreInterne, MesParamètres[4 À ])
      SINON //le champ existe
      	ChangeFenêtreSource(Onglet[ResChamp],FenetreInterne, MesParamètres[4 À ])
      FIN
     type : 458752
   -
     name : AfficheDocRéponse
     procedure_id : 1303422471619850256
     type_code : 14
     code : |1+
      procédure AfficheDocRéponse(Onglet est un champ, LibeléChamp est une chaine,FenetreInterne est une chaine <nom de fenêtre>,*)
      i			est un entier	= 1
      ResChamp	est une chaîne
      ResChamp = EnumèreChamp(Onglet, i)
      TANTQUE ResChamp <> "" 
      	SI Onglet[ResChamp].Libellé = LibeléChamp	 ALORS
      		SORTIR
      	FIN
      	i++
      	ResChamp = EnumèreChamp(Onglet, i)
      FIN
      
      SI ResChamp ="" ALORS  //aucun champ du mme nom n'a été trouvé
      	WL.OngletOuvre(Onglet,LibeléChamp,FenetreInterne, MesParamètres[4 À ])
      		
      SINON //le champ existe
      	Onglet = ResChamp
      fin
      
      
     type : 458752
   -
     name : chargerProjet
     procedure_id : 1390234206136863174
     type_code : 14
     code : |1+
      procédure chargerProjet (gnIDCourrier est un entier sur 8 octets = 0)
      
      //
      SI gnIDCourrier < 0 ALORS
      	Erreur("l'ID du courrier ("+gnIDCourrier+")n'est pas valide!")
      	Ferme()
      FIN
      SI gnIDCourrier = 0 ALORS
      	combo_etat = 1
      	COMBO_Etat.ExécuteTraitement(trtSélection)
      	COMBO_Priorité= 3
      	COMBO_etat.ExécuteTraitement(trtSélection)
      FIN
      SI gnIDCourrier > 0 ALORS  // courrier a modifier
      	SI oCourrier.SetID(gnIDCourrier)= -1 ALORS
      		Erreur ("Le courrier ayant l'ID : ("+ gnIDCourrier+") n'a pas été trouvé dans la BDD!")
      		Ferme(MoiMême)
      	FIN
      	
      	//	SAI_Réf = REQ_DonnésCourrier.Référence
      	//	SAI_Objet = REQ_DonnésCourrier.ObjetCourrier
      	//	SAI_Dte_Récep = REQ_DonnésCourrier.dateRéception
      	//	SAI_Dte_Courrier = REQ_DonnésCourrier.Datecourrier
      	//	SAI_Dte_lImite = REQ_DonnésCourrier.DateEcheance
      	
      	SI oCourrier.m_dDateEcheance <> "" ALORS
      		//INT_DateLimite = Vrai
      	FIN
      	
      	//doc courrier
      	SI oCourrier.m_nIDArchiveDocs > 0 ALORS
      		
      		DocCourrierChargé.ChargeFichierParID(oCourrier.m_nIDArchiveDocs)
      		
      		
      		SI DocCourrierChargé.GetCheminFichier() <> "" ALORS
      			afficheDocCourrier(ONG_FicheCourrier,LibOngletDocCourrier,"FI_EditeurDocuments",DocCourrierChargé.GetCheminFichier(),oCourrier.m_nIDArchiveDocs)
      			SAI_Fichier = fExtraitChemin(DocCourrierChargé.GetCheminFichier(),fFichier+fExtension)
      		SINON 
      			Info("Le fichier IDArchive : ("+ oCourrier.m_nIDArchiveDocs+") n'a pas été trouvé dans l'archive!")
      		FIN
      	FIN
      FIN
      
      SELON oCourrier.m_sEtatCourrier
      	
      	CAS "en attente" //nouveau
      		FRM_En_attente.CouleurFond = Couleur1
      	CAS "en cours" // en cours
      		FRM_En_cours.CouleurFond = Couleur2
      	CAS "en signature"
      		FRM_En_Signature.CouleurFond = Couleur3
      	CAS "en suivi"  // traité
      		FRM_En_suivi.CouleurFond = Couleur4
      	CAS "clôturé"  // traité
      		FRM_Clôturé.CouleurFond = Couleur5
      FIN
      
      
      
      
      TABLE_CourrierExp_Dest.Affiche(taInit)
      TABLE_PJ_courrier.Affiche(taInit)
      TABLE_Réponses_courrier.Affiche(taInit)
     type : 458752
