      				Destinataire_Expediteur.IDCourriers = ocourrier.m_nIDCourriers
      				Destinataire_Expediteur.IDEntitéExpDest = TABLE_CourrierExp_Dest.COL_IDEntitéExpDest
      				HAjoute(Destinataire_Expediteur)
