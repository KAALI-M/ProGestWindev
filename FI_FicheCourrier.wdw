      						SI COMBO_TypeCourrier.ValeurAffichée = "arrivé"  ALORS
      							resSauveFichier= CourrierChargé.SauveFichierDansArchive(TABLE_PJ_courrier.COL_NomDocumentPJ,"","","",COMBO_Projet.ValeurMémorisée,oCourrier.m_nIDOuvrages,TABLE_CourrierExp_Dest[1].COL_IDEntitéExpDest)
      						FIN
      						SI COMBO_TypeCourrier.ValeurAffichée = "départ" ALORS
      							resSauveFichier= CourrierChargé.SauveFichierDansArchive(TABLE_PJ_courrier.COL_NomDocumentPJ,"","","",COMBO_Projet.ValeurMémorisée,oCourrier.m_nIDOuvrages,0)				
      						FIN
      					fin
      
      					PJCourrier.IDCourriers		= oCourrier.m_nIDCourriers
      					PJCourrier.IdArchiveDocs	= resSauveFichier
      					PJCourrier.Titre			= TABLE_PJ_courrier.COL_Intitulé
      					PJCourrier.Qté				= TABLE_PJ_courrier.COL_Qté
      					
      					HModifie (PJCourrier)
