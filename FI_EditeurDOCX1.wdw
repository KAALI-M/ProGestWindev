 controls :
  -
    name : FI_EditeurDOCX1
    identifier : 0x2216b40306180527
    internal_properties : 
    controls :
     -
       name : TT_SansNom
       identifier : 0x2216b40306190527
       internal_properties : 
       type : 120
       code_elements :
        type_code : 147
        p_codes : []
    type : 32
    code_elements :
     type_code : 70
     p_codes :
      -
        code : |1-
         PROCEDURE MaFenêtre( IDArchiveDoc est un entier sur 8 octets =0,gsChemin est une chaîne = "")
         
         // cette fenêtre permet le traitement de texte de l'application 
         // en entrée elle récupère l'emplacement du fichier
         // en sortie, elle renvoie l'emplacement du fichier traité
         // gschemin doit etre "Null" pour la création d'un nouveau document
         
         
         SI gsChemin <> "" ALORS
         //	FEN_EditeurDocuments.Titre						= fExtraitChemin(gsChemin,fFichier)
         	TT_SansNom										= gsChemin
         	FI_WinDevFAA_RubanTT.BTN_WDFAA_Nouveau.Visible	= Faux
         	FI_WinDevFAA_RubanTT.BTN_WDFAA_Ouvrir.Visible	= Faux
         	FI_WinDevFAA_RubanTT.BTN_WDFAA_Save.Visible = faux
         SINON
         	TT_SansNom.Ouvre("")
         FIN
         gsRépertoire est un chaine
      -
        code : |1+
         TT_SansNom.Modifié = 0
         
        type : 14
      -
        code : |1-
         // enregisrement du Document selon qu'il est externe ou interne  IDArchiveDoc
         
         SI TT_SansNom.Modifié = 1 ALORS
         	SI OuiNon(1, "voulez-vous enregistrer le fichier modifié ? ")=Oui ALORS
         		si IDArchiveDoc >0 ALORS
         			HLitRecherchePremier(archivedocs, IdArchiveDocs, IDArchiveDoc)
         			si HTrouve(archivedocs) ALORS
         				TT_SansNom.Sauve(gsChemin)
         				TT_SansNom.Ferme()
         				HAttacheMémo(archivedocs,Document,gsChemin)
         				Si HModifie(archivedocs) alors
         				sinon
         					Erreur(HErreurInfo())
         				FIN
         			sinon
         				Erreur(HErreurInfo())
         			FIN
         		sinon
         			SI gsChemin = "" ALORS // cas d'un nouveau document
         				gsRépertoire = fSélecteur(fRepExe(), "NouveauDocument.docx", ...
         				"Sélectionner un fichier", ...
         				"Fichier (*.docx)" + TAB + "*.docx"  + TAB + "*.*", "docx", fselCrée+fselExiste)
         				TT_SansNom.Sauve(gsRépertoire)
         				TT_SansNom.Ferme()
         			SINON 
         				TT_SansNom.Sauve(gsChemin)
         				TT_SansNom.Ferme()
         			FIN
         		FIN
         		
         	FIN
         FIN
        type : 2
