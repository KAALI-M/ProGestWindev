           type : 18
     -
       name : TT_SansNom
       identifier : 0x2216b41e06576d47
       internal_properties : 
       type : 120
       code_elements :
        type_code : 147
        p_codes : []
    type : 32
    code_elements :
     type_code : 70
     p_codes :
      -
        code : |1+
         PROCEDURE MaFenêtre(gsChemin est une chaine, IDarchieDoc est un entier sur 8 octets = 0)
         
         
         TT_SansNom.Zoom											= zoomAdapteLargeur
         TT_SansNom.BarreOutilsVisible								= Faux
         DocCourrierChargé est un CFichierChargé
         
         si IDarchieDoc > 0 ALORS 
         	DocCourrierChargé.ChargeFichierParID(IDarchieDoc)
         	SI DocCourrierChargé.GetCheminFichier() <> "" ALORS
         		TT_SansNom	= DocCourrierChargé.ChemFichier
         		gsChemin =  DocCourrierChargé.ChemFichier
         		
         	SINON 
         		Info("Le fichier IDArchive : ("+ IDarchieDoc+") n'a pas été trouvé dans l'archive!")
         	FIN
         sinon
         	SI gsChemin <> "" ALORS
         		//	FEN_EditeurDocuments.Titre						= fExtraitChemin(gsChemin,fFichier)
         		TT_SansNom										= gsChemin
         		
         		FI_WinDevFAA_RubanTT.BTN_WDFAA_Nouveau.Visible	= Faux
         		FI_WinDevFAA_RubanTT.BTN_WDFAA_Ouvrir.Visible	= Faux
         		FI_WinDevFAA_RubanTT.BTN_WDFAA_Save.Visible		= Faux
         	SINON
         		TT_SansNom.Ouvre("")
         	FIN
         FIN
         
         
         fermerAvecSauvgarde, fermerSansSauvegarde est un booléen = (0,0)
         
      -
        code : |1+
         TT_SansNom.Modifié = 0
        type : 14
      -
        code : |1+
         // enregistrement du Document ?? trois fermeture forcée avec ou sans sauvegarde  est ferméture normale avec question de l'user 
         
         //2* fermeture sans sauvegarde
         si fermerAvecSauvgarde = 1 ALORS
         	sauvegardeFichier()
         sinon
         	//3* fermeture normal
         	SI fermerSansSauvegarde ALORS
         		//rein fermeture
         	sinon //demande d'action à l’utilisateur
         		SI TT_SansNom.Modifié = 1 ALORS
         			//1 : &Enregistrer
         			//2 : &Ne pas enregistrer
         			//3 : &Annuler
         			SELON Dialogue(<§?1216B90901C4A3FB§>)
         				CAS 1 
         					sauvegardeFichier()
         					TT_SansNom.Ferme()
         				CAS 2 
         				CAS 3 
         					RepriseSaisie(TT_SansNom)
         			FIN
         		SINON
         		FIN
         		RENVOYER gsChemin
         	FIN
         FIN
         
        type : 2
