  procedures :
   -
     name : estModifié
     procedure_id : 1506619178313827759
     type_code : 14
     code : |1+
      procédure global estModifié()
      
      renvoyer TT_SansNom.Modifié
      
     type : 458752
   -
     name : sauvegardeFichier
     procedure_id : 1507064149897158262
     type_code : 14
     code : |1-
      procédure global sauvegardeFichier()
      
      SI IDarchieDoc > 0 // cas de fichier ouvert par IDArchivedoc ALORS
      	TT_SansNom.Sauve(DocCourrierChargé.ChemFichier) // sauvegarde des modif vers le fichier local
      	fichierCopie est un chaine = fExtraitChemin(gsChemin,fRépertoire+fFichier)+"(1)"+fExtraitChemin(gsChemin,fExtension)
      	fCopieFichier(gsChemin,fichierCopie) // copie du fichier car le fichier ne peut être pas 
      	DocCourrierChargé.ChemFichier = fichierCopie
      	DocCourrierChargé.SauveFichierDansArchive("","","","","","","",1)  // les paramètre ne seront pas requis	
      	fSupprime (fichierCopie ) // supression du fichier crée	
      	TT_SansNom.modifié =0
      SINON
      	SI gsChemin = "" ALORS // cas d'un nouveau document
      		gsChemin = fSélecteur(fRepExe(), "NouveauDocument.docx", ...
      		"Sélectionner un fichier", ...
      		"Fichier (*.docx)" + TAB + "*.docx"  + TAB + "*.*", "docx", fselCrée+fselExiste)
      		TT_SansNom.Modifié =0
      	SINON 
      		TT_SansNom.Sauve(gsChemin)
      		TT_SansNom.Modifié = 0
      	FIN
      FIN
     type : 458752
   -
     name : fermerFichier
     procedure_id : 1507085702305558784
     type_code : 14
     code : |1+
      procédure global fermerFichier(avecSauvegarde est un booléen = 0, sansSauvegarde est un booléen = 0 )
      fermerSansSauvegarde  = sansSauvegarde
      fermerAvecSauvgarde  = avecSauvegarde
      //ferme(ParentObjet(MoiMême))
      //TT_SansNom.Ferme()
      
      
     type : 458752
