     code : |1+
      PROCEDURE MaFenêtre(gstInfo est un STInfoSerial )
      
      MaFenêtre..ValeurRenvoyée = Faux
      
      // Si la mémorisation est demandée, elle se fait dans un fichier XML dans le répertoire du profil de l'utilisateur
      gsFichierSerial est une chaine = completerep(fRepDonnéesUtilisateur())+"apionedrive.pref"
      
      // Si le fichier de mémorisation existe
      SI fFichierExiste(gsFichierSerial) ALORS
      	// On le charge et on affiche son contenu
      	bufFic est un buffer = fChargeBuffer(gsFichierSerial)
      	QUAND EXCEPTION DANS
      		Désérialise(gstInfo,bufFic,psdXML)
      		SourceVersEcran()				
      	FAIRE
      		// Si le fichier est incorrect, il est simplement ignoré
      	FIN
      FIN
      
