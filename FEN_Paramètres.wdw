     type_code : 4
     p_codes :
      -
        code : |1+
         // Vérifie que toutes les informations sont saisies
         si SAI_ID_Client~="" alors
         	ToastErreur ("Vous n'avez pas renseigné la clé publique de l'application pour accéder à l'API OVH.")
         	retour
         FIN
         si SAI_Clé_secrète~="" alors
         	ToastErreur("Vous n'avez pas renseigné la clé privée de l'application pour accéder à l'API OVH.")
         	retour
         FIN
         
         // Initialise la structure avec le contenu des champs
         EcranVersSource()
         
         // Si l'utilisateur a demandé la mémorisation des informations, elles sont sauvées dans un fichier
         si gstInfo.bMemoapp ALORS
         	stMemo est un STInfoSerial = gstInfo
         	bufSerial est un buffer
         	serialise(gstInfo,bufSerial,psdXML)
         	fSauveBuffer(gsFichierSerial,bufSerial)
         FIN
         
         ferme("",vrai)
        type : 18
