            MoiMême.Visible							= Faux
            IMG_PDFAfficheRuban.Visible					= Vrai
           type : 18
    type : 32
    code_elements :
     type_code : 70
     p_codes :
      -
        code : |1+
         PROCEDURE MaFenêtre(gsCheminPDF est une chaîne, IDarchieDoc est un entier sur 8 octets = 0)
         
         PDF_LecteurFI.Zoom											= zoomAdapteLargeur
         FI_WindevFAA_RubanLecteurPDF.BTN_WDFAA_Ouvrir.Visible	= Faux
         PDF_LecteurFI.BarreOutilsVisible								= Faux
         DocCourrierChargé est un CFichierChargé
         
         SI IDarchieDoc > 0 ALORS 
         	DocCourrierChargé.ChargeFichierParID(IDarchieDoc)
         	SI DocCourrierChargé.GetCheminFichier() <> "" ALORS
         		PDF_LecteurFI	= DocCourrierChargé.ChemFichier
         		gsCheminPDF	=  DocCourrierChargé.ChemFichier
         		
         	SINON 
         		Info("Le fichier IDArchive : ("+ IDarchieDoc+") n'a pas été trouvé dans l'archive!")
         	FIN
         SINON
         	si gsCheminPDF <> "" et fFichierExiste(gsCheminPDF) ALORS
         		PDF_LecteurFI.Ouvre(gsCheminPDF)
         	sinon
         		PDF_LecteurFI.Ferme()
         	FIN
         
         fin
